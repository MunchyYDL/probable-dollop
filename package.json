{
  "name": "devbrains-homepage",
  "private": true,
  "description": "A simple static homepage built with gatsby.",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "play": "env-cmd -f .env-development gatsby develop -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.0",
    "gatsby": "^2.24.26",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-google-fonts-v2": "^0.0.6",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gatsby-plugin-typescript": "^2.4.22",
    "gatsby-transformer-sharp": "^2.5.13",
    "gsap": "^3.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "2.0.5",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
